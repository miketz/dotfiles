# -*- mode: sh -*-

# emacs alias. leave off ".el" suffix of init file so it can choose ".elc" if exists
alias ef='emacs -nw -Q -l ~/.emacs.d/initFast'
alias e='emacs -nw -Q -l ~/.emacs.d/initFast'
alias ev='emacs -nw -Q -l ~/.emacs.d/initFastEvil'
# alias vi='vim.tiny -u NONE'
alias vi='busybox vi'
alias mg='mg -n'
